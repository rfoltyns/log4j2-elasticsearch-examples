{
  "index_patterns": [
    "log4j2-elasticsearch-hc-springboot-es-7-*"
  ],
  "settings": {
    "index": {
      "refresh_interval": "5s"
    }
  },
  "mappings": {
    "properties": {
      "trace_id": {
        "type": "keyword"
      },
      "url": {
        "type": "keyword"
      },
      "http_verb": {
        "type": "keyword"
      },
      "loggerName": {
        "type": "keyword"
      },
      "message": {
        "type": "text"
      },
      "timeMillis": {
        "type": "date",
        "format": "epoch_millis"
      },
      "thread": {
        "type": "keyword"
      },
      "level": {
        "type": "keyword"
      }
    }
  }
}
